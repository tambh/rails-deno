<% @users.each do |user| %>
<tr>
	<td><%= user.name %></td>
	<td><%= user.email %></td>
	<td><%= link_to 'Show', user %></td>
	<td><%= link_to 'Edit', edit_user_path(user) %></td>
	<td><% if current_user.admin? && !current_user?(user) %>
	| <%= link_to "delete", user, method: :delete,
	data: { confirm: "You sure?" } %>
	<% end %></td>
</tr>
<% end %>
